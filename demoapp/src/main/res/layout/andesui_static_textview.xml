            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:background="@color/andes_accent_color"
            android:text="AndesTextView Inverted color with link"
            app:andesTextViewIsLinkColorInverted="true"
            app:andesTextViewStyle="bodyM"
            app:andesTextViewTextColor="inverted"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/static_page_textview_range_positive_link" />

        <com.mercadolibre.android.andesui.textview.AndesTextView
            android:id="@+id/static_page_textview_range_primary_bold"
