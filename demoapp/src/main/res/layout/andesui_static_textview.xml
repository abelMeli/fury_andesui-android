            android:layout_marginTop="12dp"
            android:text="AndesTextView Primary color with bold"
            app:andesTextViewStyle="bodyM"
            app:andesTextViewTextColor="primary"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/static_page_textview_range_inverted_link" />

        <com.mercadolibre.android.andesui.textview.AndesTextView
            android:id="@+id/static_page_textview_range_positive_bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            android:text="AndesTextView Positive color with bold"
            app:andesTextViewStyle="bodyM"
            app:andesTextViewTextColor="positive"
            app:layout_constraintStart_toStartOf="parent"
