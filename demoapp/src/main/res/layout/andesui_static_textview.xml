            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:text="Multilink Example"
            app:andesTextViewStyle="titleM"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/static_page_textview_range_caution_bold_and_link" />

        <com.mercadolibre.android.andesui.textview.AndesTextView
            android:id="@+id/static_page_textview_example_multiple_links"
