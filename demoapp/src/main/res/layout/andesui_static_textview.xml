            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="24dp"
            android:layout_marginEnd="12dp"
            android:text="Para poder acceder a este beneficio, debes tener en cuenta las especificaciones y los términos y condiciones que se disponen. Además el CUIT que figura en la sección Mi Perfil debe estar actualizado."
            app:layout_constrainedWidth="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/static_page_textview_multilink_example" />

        <com.mercadolibre.android.andesui.textview.AndesTextView
            android:id="@+id/static_page_textview_moneyamount_example"
