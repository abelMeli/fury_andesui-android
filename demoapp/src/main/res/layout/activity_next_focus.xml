    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:nextFocusRight="@id/nf_button_8"
        android:nextFocusUp="@id/nf_button_4"
        android:nextFocusDown="@id/nf_button_10"
        android:nextFocusForward="@id/nf_button_8"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_7"
        app:layout_constraintEnd_toStartOf="@id/nf_button_8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nf_button_4" />

    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:nextFocusLeft="@id/nf_button_7"
        android:nextFocusRight="@id/nf_button_9"
        android:nextFocusUp="@id/nf_button_5"
        android:nextFocusDown="@id/nf_button_11"
        android:nextFocusForward="@id/nf_button_9"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_8"
        app:layout_constraintEnd_toStartOf="@id/nf_button_9"
        app:layout_constraintStart_toEndOf="@id/nf_button_7"
        app:layout_constraintTop_toBottomOf="@id/nf_button_4" />

    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:nextFocusLeft="@id/nf_button_8"
        android:nextFocusUp="@id/nf_button_6"
        android:nextFocusDown="@id/nf_button_12"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_9"
        app:layout_constraintEnd_toEndOf="parent"
