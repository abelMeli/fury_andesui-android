        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/nf_button_11"
        app:layout_constraintTop_toBottomOf="@id/nf_tv_code" />

    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_13"
        app:layout_constraintEnd_toStartOf="@id/nf_button_11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/nf_button_10" />

    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_14"
        app:layout_constraintEnd_toStartOf="@id/nf_button_12"
        app:layout_constraintStart_toEndOf="@id/nf_button_10"
        app:layout_constraintTop_toBottomOf="@id/nf_button_10" />

    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_15"
        app:layout_constraintEnd_toEndOf="parent"
