    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_17"
        app:layout_constraintEnd_toStartOf="@id/nf_button_12"
        app:layout_constraintStart_toEndOf="@id/nf_button_10"
        app:layout_constraintTop_toBottomOf="@id/nf_button_13" />

    <com.mercadolibre.android.andesui.button.AndesButton
        android:id="@+id/nf_button_18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:andesButtonText="@string/andes_demoapp_playground_ata_button_18"
