<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="AndesSwitch" tools:ignore="ResourceName">

        <attr name="andesSwitchAlign" format="enum">
            <enum name="left" value="1000" />
            <enum name="right" value="1001" />
        </attr>

        <attr name="andesSwitchStatus" format="enum">
            <enum name="checked" value="2000" />
            <enum name="unchecked" value="2001" />
        </attr>

        <attr name="andesSwitchType" format="enum">
            <enum name="enabled" value="3000" />
            <enum name="disabled" value="3001" />
        </attr>

        <attr name="andesSwitchText" format="reference" />

    </declare-styleable>

</resources>