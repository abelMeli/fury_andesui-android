<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AndesInputStepper">

        <attr name="andesInputStepperSize">
            <enum name="large" value="1000" />
            <enum name="small" value="1001" />
        </attr>

        <attr name="andesInputStepperStatus">
            <enum name="idle" value="2000" />
            <enum name="disabled" value="2001" />
            <enum name="loading" value="2002" />
        </attr>

        <attr name="andesInputStepperValue" format="integer" />

        <attr name="andesInputStepperMaxValue" format="integer" />

        <attr name="andesInputStepperMinValue" format="integer" />

        <attr name="andesInputStepperStep" format="integer" />

    </declare-styleable>
</resources>