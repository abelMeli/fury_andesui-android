<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="AndesCarousel" tools:ignore="ResourceName">
        <attr name="andesCarouselCenter" format="boolean"/>
        <attr name="andesCarouselMargin">
            <enum name="none" value="100" />
            <enum name="defaultMargin" value="101" />
            <enum name="xsmall" value="102" />
            <enum name="small" value="103" />
            <enum name="medium" value="104" />
            <enum name="large" value="105" />
        </attr>
        <attr name="andesCarouselInfinite" format="boolean"/>
        <attr name="andesCarouselAutoplay" format="boolean"/>
        <attr name="andesCarouselAutoplaySpeed" format="integer"/>
        <attr name="andesCarouselTitle" format="reference"/>
        <attr name="andesCarouselPaginator" format="boolean"/>
    </declare-styleable>
</resources>